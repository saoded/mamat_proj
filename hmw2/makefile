# Makefile automatically generated by MakeMake for 044101 - HW3
CC = gcc
CFLAGS = -g -Wall
CCLINK = $(CC) $(CFLAGS)
LIBS = 
OBJS = airport.o flight.o main.o runway.o
RM = rm -f
# Create the executable
airport: $(OBJS)
	$(CCLINK) -o airport $(OBJS) $(LIBS)
# Create object files
airport.o: airport.c airport.h
	$(CCLINK) -c airport.c
flight.o: flight.c flight.h
	$(CCLINK) -c flight.c
main.o: main.c
	$(CCLINK) -c main.c
runway.o: runway.c runway.h
	$(CCLINK) -c runway.c

# Cleaning old files before new make
clean:
	$(RM) airport *.o *.bak *~ "#"* core

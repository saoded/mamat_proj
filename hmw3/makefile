# Makefile automatically generated by MakeMake for 044101 - HW3
CC = gcc
CFLAGS = -g -Wall
CCLINK = $(CC) $(CFLAGS)
LIBS = 
OBJS = cluster.o list.o main.o person.o point.o
RM = rm -f
# Create the executable
prog: $(OBJS)
	$(CCLINK) -o prog $(OBJS) $(LIBS)
# Create object files
cluster.o: cluster.c cluster.h point.h list.h defs.h
	$(CCLINK) -c cluster.c
list.o: list.c list.h defs.h
	$(CCLINK) -c list.c
main.o: main.c list.h defs.h point.h cluster.h person.h
	$(CCLINK) -c main.c
person.o: person.c person.h defs.h list.h
	$(CCLINK) -c person.c
point.o: point.c point.h list.h defs.h
	$(CCLINK) -c point.c

# Cleaning old files before new make
clean:
	$(RM) prog *.o *.bak *~ "#"* core

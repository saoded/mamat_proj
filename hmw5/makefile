OBJS = Commands.o LineParser.o main.o MamatScriptEngine.o Matrix.o Scalar.o Variable.o VariablesMap.o
RM = rm -f
# Create the executable
MamatScript: $(OBJS)
	$(CCLINK) -o MamatScript $(OBJS) $(LIBS)
